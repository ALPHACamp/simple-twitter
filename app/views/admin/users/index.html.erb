<div class="comtainer">
  <div class="row">
    <div class="col-md-10 col-md-offset-2">

      <ul class="nav nav-tabs">
        <li role="presentation" class=<%= "active" if current_page? admin_tweets_path %>>
          <%= link_to "Tweets", admin_tweets_path %>
        </li>
        <li role="presentation" class=<%= "active" if current_page? admin_users_path %>>
          <%= link_to "Users", admin_users_path %>
        </li>
      </ul>

      <hr>

        <h3>Tweets list</h3>
        <% @users.each do |user|%>
          <div>
            <h4><%= link_to user.name, user_path(user)%></h4>
            email: <br>
            Tweets.size: <br>
            Following.size: <br>
            Followers.size: <br>
            liked.size: 
          </div>
          <hr>
        <% end %>
  

    </div>
  </div>
</div>