<div class="container-1">

  <div class= "tweet-btn col-mb-1">

    <%= link_to "User List", admin_users_path, class: "btn-md btn-info active" %>
  </div>
  <div class="row col-md-4">
  <div class= "tweet-btn col-md-4">

    <%= link_to "Tweets", admin_tweets_path, class: "btn-md btn-info active" %>
  </div>

</div>
  </div>
  <div class="container-2">
<table class="table justify-content-md-center mb-3">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Tweets</th>
      <th>Followings</th>
      <th>Followers</th>
      <th>likes</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.tweets.count %></td>
        <td><%= user.followings.count %></td>
        <td><%= user.followers_count %></td>
        <td><%= user.likes_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= render partial: "shared/backhome" %>
</div>
</div>
