<div class="container">
  <div class="row">
    <div class="col-sm-3">
      <%= render partial: "shared/user_dashboard", locals: {user: @user} %>
    </div>

    <div class="col-sm-9">
    <h2>喜歡的推播清單</h2>
    <% @likes.each do |like| %>
      <%= render partial: "shared/tweet_wall", locals: {tweet: like} %>
    <% end %>
    </div>
  </div>
</div>
