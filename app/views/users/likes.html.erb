<!-- user's likes -->
<div class="container">
  <div class="row">
    <div class="col-xs-2">
      

      <%= render partial: "shared/user_section", locals: {user: @user} %>
    </div>

    <div class="col-xs-9 col-xs-offset-1">
      <h3>Like</h3>
      <div>
        <% @likes.each do |like| %>
          <%= render partial: "shared/tweet_list", locals: {tweet: like.tweet} %>
        <% end %>
      </div>
    </div>
  </div>
</div>