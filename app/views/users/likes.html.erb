<div class="row">
  
  <div class="col-md-3">
    <%= render "shared/profile", user: @user %>
  </div>

  <div class="col-md-8 col-md-offset-1">
    
    <h2>Likes</h2>

    <div class="col-md-12">
      <% @liked_tweets.each do |tweet| %>
        <%= render "shared/media", tweet: tweet %>
      <% end %>      
    </div>

  </div>

</div>