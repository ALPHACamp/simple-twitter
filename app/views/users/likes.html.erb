<div class="container">
  <div class="index">
    <%= render partial: "share/user_item", locals: {user: @user} %>
    <div id="tweet-wall">
      <h2>Like</h2>
      <% @likes.each do |tweet| %>
        <%= render partial: "share/tweet_item", locals: {tweet: tweet} %>
      <% end %>
    </div>    
  </div>
</div>