<div class="container-fluid">
  <div class="row">
    <div class="col-md-4">
      <%= render partial: "shared/userProfile", locals: {user: @user} %>
    </div>
    <div class="col-md-8">
      <% @likes.each do |like| %>
         <%= render partial: "shared/tweetRecord", locals: {tweet: like.tweet} %>
      <% end %>
    </div>
  </div>
</div>