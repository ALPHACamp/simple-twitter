<div class="container">
  <div class="row">
    <%= render partial: "shared/userinfo", locals: {user: @user} %>
    <div class="col-md-8 col-md-offset-1">
      <h1>Like</h1>
      <div class="col-md-12">
        <% @likes.each do |like| %>
          <%= render partial: "shared/tweet", locals: {tweet: like} %>
        <% end %>
      </div>
    </div>
  </div>
</div>
