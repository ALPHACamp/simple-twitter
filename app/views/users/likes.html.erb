<div class="container">
<div class="container">


  <%# render 'shared/profile', locals: { user: @user} %>
  <!-- user info panel -->
  <div class="users col-md-5" >

    <%=render partial: "/shared/profile", locals:{user: @user}%>
  </div>

  <div class ="tweets col-md-7" >
    <!-- user's tweetwall -->
    <h1>Likes</h1>
        <% @likes.each do |tweet| %>
         <%= render partial: "shared/message", locals: {message: tweet, message_content: tweet.description, message_btn: true} %>
        <% end %>

  </div>

</div>
<%= render partial: "shared/backhome" %>
</div>
