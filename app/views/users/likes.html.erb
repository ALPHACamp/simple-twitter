<div class="container">
  <div class="row">

    <div class="col-md-3 user-profile">
      <%= render partial: "shared/user_profile", locals: {user: @user} %>
    </div>

    <div class="col-md-8">
      <h2>Like</h2>
      <% @likes.each do |like| %>
        <%= render partial: "shared/tweet", locals: {tweet: like} %>
      <% end %>
    </div>

  </div>
</div>