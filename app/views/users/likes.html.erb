<div class="row">
  <!-- user profile -->
  <div class="col-xs-12 col-sm-3">
    <%= render partial: "shared/user_data" %>
  </div>

  <!-- tweets -->
  <div class="col-xs-12 col-sm-9 likes-col">
    <hr class="hidden-sm hidden-md hidden-lg">

    <h1>Like</h1>
    <% @likes.each do |like| %>
      <%= render partial: "shared/message", locals: {message: like, is_tweet: true} %>
    <% end %>
    <div class="text-center"><%= paginate @likes %></div>

  </div>
</div>