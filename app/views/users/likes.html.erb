<div class="container">
  <div class="row">
    <div class="col-md-4">
      <%= render partial: "shared/userinfo", locals: {user: @user} %>
    </div>
    <div class="col-md-8">
      <h3></strong>Like</h3>
      <% @likes.each do |tweet| %>
        <%= render partial: "shared/tweet", locals: {tweet: tweet} %>
      <% end %>
    </div>
  </div>
</div>
