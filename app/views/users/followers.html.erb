  <div class="container">
     <div class="col-md-3">
      
        <%= render partial: "shared/profile", locals: {user: @user} %>
     
    </div>
    <div class="col-md-9">
     <div class='row'>
      <h2>Followers</h2>
      
        <% @followers.each do |follower| %>
         <div class="user-list-box col-sm-6 col-md-4">
          <%= render partial: "shared/userlist", locals: {follow: follower} %>
       </div>
        <% end %>
      
      </div>
    </div>
</div>
