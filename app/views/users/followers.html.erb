<div class="row">
  <div class="col-md-4 col-sm-12">
  	<%= render partial: "shared/profile", locals:{user: @user}%>
  </div>
  <div class="col-md-8 col-sm-12">
    <div class="divtitle"><h3>Followers</h3></div>
  	<div class="profile-blocks-container">
  		<% @followers.each do |u| %>
  		<div class="profile-flexblock">
  			<%= render partial: "shared/oneuser", locals:{user: u}%>
  		</div>
  		<%end%>
  	</div>
  </div>