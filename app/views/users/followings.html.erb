<div class="container">

  <div class="row">
    <%= render partial: "shared/user_data" %> 

    <div class="col-8">
      <h3>Following</h3>
      
      <div class="row">
        <% @followings.each do |following| %>          
          <%= render partial: "shared/follow_list", locals: {follow: following} %>         
        <% end %>
      </div>

    </div>
  
  </div>
  
</div>
