<div class="row">
  <!-- user profile -->
  <div class="col-xs-3">
    <%= render partial: "shared/user_data" %>
  </div>

  <!-- Followings -->
  <div class="col-xs-9">
    <h1>Followings</h1>
    <div class="row">
      <% @followings.each do |followings| %>
        <div class="col-xs-6">
          <%= render partial: "shared/user_card", locals: {user: followings} %>
        </div>
      <% end %>
    </div>
  </div>

</div>